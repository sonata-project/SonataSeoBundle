{
    "name": "sonata-project/seo-bundle",
    "description": "Symfony SonataSeoBundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": [
        "seo",
        "sonata"
    ],
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "https://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataSeoBundle/contributors"
        }
    ],
    "homepage": "https://docs.sonata-project.org/projects/SonataSeoBundle",
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "sonata-project/block-bundle": "^4.0",
        "sonata-project/exporter": "^2.0 || ^3.0",
        "symfony/config": "^4.4 || ^5.4 || ^6.0",
        "symfony/dependency-injection": "^4.4 || ^5.4 || ^6.0",
        "symfony/form": "^4.4 || ^5.4 || ^6.0",
        "symfony/framework-bundle": "^4.4 || ^5.4 || ^6.0",
        "symfony/http-foundation": "^4.4 || ^5.4 || ^6.0",
        "symfony/http-kernel": "^4.4.13 || ^5.4 || ^6.0",
        "symfony/options-resolver": "^4.4 || ^5.4 || ^6.0",
        "twig/twig": "^2.9 || ^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4",
        "knplabs/knp-menu": "^3.0",
        "knplabs/knp-menu-bundle": "^3.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.0",
        "phpstan/phpstan-symfony": "^1.0",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.17",
        "rector/rector": "^0.13",
        "symfony/console": "^4.4 || ^5.4 || ^6.0",
        "symfony/filesystem": "^4.4 || ^5.4 || ^6.0",
        "symfony/finder": "^4.4 || ^5.4 || ^6.0",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/yaml": "^4.4 || ^5.4 || ^6.0",
        "vimeo/psalm": "^4.7.2"
    },
    "autoload": {
        "psr-4": {
            "Sonata\\SeoBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sonata\\SeoBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    }
}
